{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "default-case": 0,
        "no-console": "off",
        "no-unused-vars": "off",
        "func-names": ["error", "never"]
    }
    
  }